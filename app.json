{
  "expo": {
    "name": "Kuchenland Logistic",
    "description":"QR code scaner and package manager",
    "slug": "Kuchenland-Logistic",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": [
      "android"
    ],
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#fa000c"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.Kuchenland.logisticservice",
      "permissions":[ "CAMERA"]
    }
  }
}